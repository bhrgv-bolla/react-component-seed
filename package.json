{
  "name": "seed-component",
  "version": "0.1.0",
  "description": "<%= componentName %> - React component",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "prepublish": "builder run build",
    "build": "builder run build",
    "start": "builder run serve:playground",
    "tdd": "builder run test:all:watch",
    "pretest": "builder run lint",
    "test": "builder run test:all:coverage",
    "lint": "builder run lint",
    "release": "builder run release"
  },
  "repository": {
    "type": "git",
    "url": "git+<%= repoUrl %>"
  },
  "keywords": [
    "react",
    "component",
    "<%= componentName %>",
    "seed"
  ],
  "author": "<%= author %>",
  "license": "MIT",
  "bugs": {
    "url": "<%= repoUrl %>/issues"
  },
  "homepage": "<%= repoUrl %>",
  "devDependencies": {
    "builder": "^2.9.1",
    "builder-react-comp-dev": "~0.0.6"
  },
  "dependencies": {
    "builder-react-comp": "~0.0.6"
  }
}
